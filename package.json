{
  "name": "angular-starter-v6",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev",
    "serve:dev": "ng serve --disable-host-check --port 4200 -o",
    "serve:prod": "ng serve --prod --disable-host-check --port 4200 -o",
    "build": "npm run build:prod",
    "build:prod": "ng build --prod -bh /FrontDemo/",
    "build:dev": "ng dev --dev -bh /FrontDemo/",
    "doc": "npm run doc:serve",
    "doc:build": "compodoc -p src/tsconfig.app.json --theme \"Original\" --hideGenerator -n \"Starter v2 Documentation\" -d \"Documentaion/\" --customFavicon src/favicon.ico -t",
    "doc:serve": "compodoc -p src/tsconfig.app.json --theme \"Original\" -n \"Starter v2 Documentation\" -d \"Documentaion/\" --customFavicon src/favicon.ico  -r 4250 -s -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "bootstrap": "^4.0.0-beta.3",
    "core-js": "^2.5.4",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "ng-block-ui": "^1.0.6",
    "oidc-client": "^1.4.1",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2",
    "sweetalert2": "^7.12.2",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@compodoc/compodoc": "^1.0.6",
    "@types/bootstrap": "^3.3.38",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.2.18",
    "@types/js-cookie": "^2.1.0",
    "@types/lodash": "^4.14.92",
    "@types/node": "~8.9.4",
    "angular-in-memory-web-api": "^0.6.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
